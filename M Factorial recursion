import java.io.*;
import java.util.Scanner;
class sathya
{
    public static void factorial()
    {
        int fact=1;
        System.out.print("enter the number=");
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        for(int i=1;i<=n;i++)
        fact=fact*i;
        System.out.print(fact);
    }
    public static void main(String[]args)
    {
        factorial();
    }
}
